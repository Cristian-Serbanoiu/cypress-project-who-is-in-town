{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["esnext", "dom"],
    "module": "commonjs",
    "strict": true,
    "baseUrl": "./",
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["cypress", "node"]
  },
  "include": ["cypress/**/*.ts"]
}
